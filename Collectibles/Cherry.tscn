[sub_resource type="GDScript" id="GDScript_hyq7q"]
script/source = "extends Area2D

var given_rewards = false

func _on_body_entered(body):
	if body.name == \"Player\":
		given_rewards = true
		body.gold += 1
		body.playerHP += 2
		if body.playerHP > body.player_max_hp:
			body.playerHP = body.player_max_hp
		var tween = get_tree().create_tween()
		var tween1 = get_tree().create_tween()
		tween.tween_property(self, \"position\", position - Vector2(0,35), 0.4)
		tween1.tween_property(self, \"modulate:a\", 0, 0.35)
		tween.tween_callback(queue_free)


"

